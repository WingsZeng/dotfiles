host smtp.gmail.com
port 587
from wings.xiangyi.zeng@gmail.com
user wings.xiangyi.zeng@gmail.com
passwordeval "pass wings.xiangyi.zeng@gmail.com"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/wings/.config/msmtp/msmtp.log
# tls_starttls

